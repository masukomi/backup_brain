<div class="container">
  <div class="row">
    <div class="col sidebar-tags">
      <% if defined?(@tags) %>
        <div id="tags-list-heading"><%= t('tags.overlapping_tags') %></div>
      <% end %>
      <% @tags_list.each do |tag| %>
        <%= link_to(tag, bookmarks_tagged_with_path(tags: tag)) %>
      <% end %>
    </div>
  </div>
</div>
